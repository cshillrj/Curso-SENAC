@model List<Usuario>

@{
    ViewData["Title"] = "Lista de Usuarios";
}

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nome</th>
                <th>Login</th>
                <th>Senha</th>
                <th>Data de Nascimento</th>
                <th>Operações</th>
            </tr>
        </thead>

            @foreach (Usuario u in Model)
            {
                <tr>
                    <td>@u.Id</td>
                    <td>@u.Nome</td>
                    <td>@u.Login</td>
                    <td>@u.Senha</td>
                    <td>@u.DataNasc</td>
                    <td>
                        <a href="/Usuario/Atualizar?Id=@u.Id">Alterar</a>

                        <a href="/Usuario/Excluir?Id=@u.Id">Excluir</a>
                </tr>
            }
        <tbody>
        
        </tbody>
    </table>

    