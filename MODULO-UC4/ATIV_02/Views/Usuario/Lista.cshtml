@model List<Usuario>

@{
    ViewData["Title"] = "Lista de Usuarios";
}

<h2>Lista de Usuários</h2>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nome</th>
                <th>Login</th>
                <th>Senha</th>
                <th>Data de Nascimento</th>
                <th>Operações</th>
            </tr>
        </thead>

            @foreach (Usuario u in Model)
            {
                <tr>
                    <td>@u.Id</td>
                    <td>@u.Nome</td>
                    <td>@u.Login</td>
                    <td>@u.Senha</td>
                    <td>@u.DataNasc</td>
                </tr>
            }
        <tbody>
        
        </tbody>
    </table>

    